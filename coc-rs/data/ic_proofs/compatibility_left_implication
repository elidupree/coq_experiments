A1. (A = B) -> (A C = B C)
A2. ((A, B) = (C, D)) = ((A = C) & (B = D))

1. (A = B) -> ((A C) = (B C))
2. (A = B) = ((A, (A True)) = (B, (B True)))            [A2]
3. ((A, (A True)) = (B, (B True))) -> ((A, (A True))(x => xc => (C x)) = (B, (B True))(x => xc => (C x)))   [A1]
4. ((A, (A True)) = (B, (B True))) -> ((C A) = (C B))   [by unfolding]
5. (A = B) -> ((C A) = (C B))                           [by subst]