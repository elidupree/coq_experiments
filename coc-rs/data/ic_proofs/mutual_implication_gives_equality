#P1. (A=B) -> (C=D)
#P2. (C=D) -> (A=B)

#1.



#A1. ((A = B) & (B = C)) -> (A = C)

#. (((A=B) -> (C=D)) & ((C = D) = ((C,A) = (D,B))) -> ((A=B) = (C=D))
#. (((A=B) -> (C=D)) & (((C,A) = (D,B)) = (C=D)) -> ((A=B) = (C=D))
#. (((A=B) -> (C=D)) & (((C=D) & (A=B)) = (C=D)) -> ((A=B) = (C=D))
#. (((A=B) -> (C=D)) & ((C=D) -> (A=B))) -> ((A=B) = (C=D))


#or

P1. (A=B) = ((A,C)=(B,D))
P2. (C=D) = ((C,A)=(D,B))

1. ((A,C)=(B,D)) = ((C,A)=(D,B))
3. (A=B) = (C=D)